{
  "semi": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "tabWidth": 4,
  "useTabs": true,
  "singleQuote": true,
  "overrides": [{
      "files": ["*.json", ".eslintrc", ".tslintrc", ".prettierrc", ".tern-project"],
      "options": {
        "parser": "json",
        "tabWidth": 4
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}